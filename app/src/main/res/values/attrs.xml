<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IndexBar">
        <attr name="indexTextSize" format="dimension" />
        <attr name="indexTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="TagFlowLayout">
        <attr name="auto_select_effect" format="boolean"></attr>
        <attr name="max_select" format="integer"></attr>
        <attr name="gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>
    <declare-styleable name="day">
        <attr name="state_current" format="boolean" />
    </declare-styleable>

    <attr name="calendarViewStyle" format="reference" />
</resources>